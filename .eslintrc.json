{
	"extends": "airbnb-base",
	"rules": {
		"no-console": "off",
		"max-len": 0,
		"consistent-return": 0,
		"func-names": 0,
		"no-plusplus": 0,
		"no-param-reassign": 0,
		"import/no-dynamic-require": 0,
		"no-restricted-syntax": 0,
		"guard-for-in": 0,
		"no-prototype-builtins": 0,
		"no-undef": 0,
		"prefer-spread": 0,
		"prefer-rest-params": 0,
		"no-mixed-operators": 0,
		"no-bitwise": 0,
		"no-loop-func": 0,
		"no-return-assign": 0,
		"no-unused-vars": [
			2,
			{
				"args": "after-used",
				"argsIgnorePattern": "it"
			}
		]
	}
}